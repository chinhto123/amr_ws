<launch>
  <include
    file="$(find gazebo_ros)/launch/empty_world.launch" />
 
  <node
    name="spawn_model"
    pkg="gazebo_ros"
    type="spawn_model"
    args="-x 0 -y 0 -z 0.1 -file $(find autoxing)/urdf/autoxing.urdf -urdf -model autoxing"
    output="screen" />
  <node
    name="fake_joint_calibration"
    pkg="rostopic"
    type="rostopic"
    args="pub /calibrated std_msgs/Bool true" />
</launch>
